a, b, c, d = map(int, input().split())
 
possible = (
 
    a + b - c == d or
    a + b * c == d or
    a - b + c == d or
 
    a - b * c == d or
    a * b + c == d or
    a * b - c == d or
 
    (a + b) * c == d or
    (a - b) * c == d or
    a * (b + c) == d or
    a * (b - c) == d or
    (a + b) - c == d or
    (a - b) + c == d
)
 
print("YES" if possible else "NO")
